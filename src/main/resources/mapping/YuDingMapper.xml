    from yuding_t where 1=1
   <!--and jibie = #{jiBie,jdbcType=VARCHAR} and rzSj <= #{rzSj,jdbcType=TIMESTAMP} and date_add(rzSj, interval rzts day)>= #{rzSj,jdbcType=TIMESTAMP} --> 
